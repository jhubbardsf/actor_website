= form_for @admin_video, :html => { :multipart => true } do |f|
  - if @admin_video.errors.any?
    #error_explanation
      %h2= "#{pluralize(@admin_video.errors.count, "error")} prohibited this admin_video from being saved:"
      %ul
        - @admin_video.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :title
    = f.text_field :title
  .field
    = f.label :user_id
    = f.number_field :user_id
  -#.field
  -#  = f.label :video_file
  -#  = f.file_field :video_file
  .actions
    = f.submit 'Save'
